{
  "name": "minimaldirectives",
  "version": "1.0.0",
  "description": "",
  "main": "src/js/index.js",
  "scripts": {
    "build": "npm run clean && npm run dir && npm run js && npm run copy",
    "build:watch": "chokidar \"src/js/**/*.js\" -c \"npm run build\"",
    "dev": "npm run serve | npm run js:watch | npm run copy:watch",
    "clean": "rimraf dist",
    "lint": "eslint src/js/index.js",
    "js": "webpack --config webpack.config.js",
    "js:watch": "chokidar \"src/**/*.js\" -c \"npm run js\"",
    "copy": "cp src/index.html dist/index.html | cp src/page.html dist/page.html | cp -r src/static dist",
    "copy:watch": "chokidar \"src/**/*.html\" -c \"npm run copy\"",
    "dir": "mkdirp dist/js",
    "serve": "node ./task/serve dist 8385"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-babili": "0.0.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.17.1",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.19.0",
    "express": "^4.15.2",
    "mkdirp": "^0.5.1",
    "open": "0.0.5",
    "rimraf": "^2.6.1",
    "serve-static": "^1.12.1",
    "webpack": "^2.4.1"
  },
  "author": "Ron Valstar",
  "license": "GPL-3.0",
  "dependencies": {
    "gsap": "^1.20.2",
    "matter-js": "^0.13.0",
    "signals": "^1.0.0",
    "voronoi": "^1.0.0"
  }
}
